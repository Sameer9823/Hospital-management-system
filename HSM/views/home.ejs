<style>
  /* Global Styles */



.lead {
  color: #aaa;
}

.wrapper {
  margin: 10vh 0;
}

/* Card Styles */
.card {
  border: none;
  border-radius: 20px;
  box-shadow: 0 0 12px 0 rgba(0, 0, 0, 0.2);
  min-height: 450px;
  overflow: hidden;
  transition: all 500ms cubic-bezier(0.19, 1, 0.22, 1);
}

@media (max-width: 768px) {
  .card {
    min-height: 350px;
  }
}

@media (max-width: 420px) {
  .card {
    min-height: 300px;
  }
}

/* Background & Hover Effect */
.card-has-bg {
  background-size: 120%;
  background-repeat: no-repeat;
  background-position: center center;
  position: relative;
}

.card-has-bg:before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: inherit;
  filter: grayscale(100%);
}

.card-has-bg:hover {
  transform: scale(0.98);
  box-shadow: 0 0 5px -2px rgba(0, 0, 0, 0.3);
  background-size: 130%;
}

.card-has-bg:hover .card-img-overlay {
  background: linear-gradient(0deg, rgba(255, 186, 33, 0.5) 0%, #21fbff 100%);
}

.card-img-overlay {
  transition: all 800ms cubic-bezier(0.19, 1, 0.22, 1);
  background: linear-gradient(0deg, rgba(255, 186, 33, 0.38) 0%, #21fbff 100%);
}

/* Card Body */
.card-body {
  transition: all 500ms cubic-bezier(0.19, 1, 0.22, 1);
}

.card-has-bg:hover .card-body {
  margin-top: 30px;
}

.card-title {
  font-weight: 800;
}

.card-meta {
  color: rgba(0, 0, 0, 0.3);
  text-transform: uppercase;
  font-weight: 500;
  letter-spacing: 2px;
}

/* Footer */
.card-footer {
  background: none;
  border-top: none;
}

.media img {
  border: solid 3px rgba(255, 255, 255, 0.3);
}


</style>

