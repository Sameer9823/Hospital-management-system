    <style>
      /* Global Styles */

      :root {
  --surface-color: #fff;
  --primary-color: #ff6f61; /* Enhancing with a primary color */
  --secondary-color: #d7bdca; 
  --curve: 40;
  --shadow-color: rgba(0, 0, 0, 0.2);
}

* {
  box-sizing: border-box;
}

body {
  font-family: 'Noto Sans JP', sans-serif;
  background-color: #fef8f8;
 
}

.cards {
  display: grid;
  grid-template-columns: repeat(3, 1fr); /* Ensures 3 cards per row */
  gap: 2rem; /* Space between cards */
  margin: 4rem 5vw;
  padding: 0;
  list-style-type: none;
}


.card {
  position: relative;
  display: block;
  height: 100%;
  border-radius: calc(var(--curve) * 1px);
  overflow: hidden;
  text-decoration: none;
  box-shadow: 0 10px 20px var(--shadow-color); /* Adding shadow */
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.card:hover {
  transform: translateY(-10px);
  box-shadow: 0 15px 30px rgba(0, 0, 0, 0.25);
}

.card__image {
  width: 100%;
  height: auto;
  transition: transform 0.5s ease;
}

.card:hover .card__image {
  transform: scale(1.05);
}

.card__overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1;
  border-radius: calc(var(--curve) * 1px);
  background-color: var(--surface-color);
  transform: translateY(100%);
  transition: .3s ease-in-out;
}

.card:hover .card__overlay {
  transform: translateY(0);
}

.card__header {
  position: relative;
  display: flex;
  align-items: center;
  gap: 2em;
  padding: 2em;
  border-radius: calc(var(--curve) * 1px) 0 0 0;
  background-color: var(--surface-color);
  transform: translateY(-100%);
  transition: .3s ease-in-out;
}

.card__arc {
  width: 80px;
  height: 80px;
  position: absolute;
  bottom: 100%;
  right: 0;
  z-index: 1;
}

.card__arc path {
  fill: var(--surface-color);
  d: path("M 40 80 c 22 0 40 -22 40 -40 v 40 Z");
}

.card:hover .card__header {
  transform: translateY(0);
}

.card__thumb {
  flex-shrink: 0;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  border: 2px solid var(--primary-color); /* Make the thumbnail stand out */
}

.card__title {
  font-size: 1.2em;
  margin: 0 0 .3em;
  color: #6b6bff; /* Better visibility with a bright color */
  font-weight: bold;
}

.card__tagline {
  display: block;
  margin: 1em 0;
  font-family: 'Noto Sans JP', sans-serif;
  font-size: .9em;
  color: var(--secondary-color);
}

.card__status {
  font-size: .8em;
  color: var(--secondary-color);
}

.card__description {
  padding: 0 2em 2em;
  margin: 0;
  color: var(--secondary-color);
  font-family: 'Noto Sans JP', sans-serif;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  transition: color 0.3s ease;
}

.card:hover .card__description {
  color: #333; /* Darker color on hover for better readability */
}


